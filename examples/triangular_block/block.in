# First test

read triangl_mesh block_single_very_fine.msh
info triangl_mesh

set triangl_mesh bound_value buttom 300
set triangl_mesh bound_value right 100
set triangl_mesh bound_value top 300
set triangl_mesh bound_value left 100

set triangl_mesh bound_type buttom dirichlet
set triangl_mesh bound_type right dirichlet
set triangl_mesh bound_type top dirichlet
set triangl_mesh bound_type left dirichlet

set writer triangl_mesh vtk on
set writer triangl_mesh vtk file_name cube_triangl

set solver mesh_type triangular

run
