# First test

read triangl_mesh pipe_fine.msh
info triangl_mesh

set triangl_mesh bound_value outer 300
set triangl_mesh bound_value inner 100
set triangl_mesh bound_value cut 0

set triangl_mesh bound_type outer dirichlet
set triangl_mesh bound_type inner dirichlet
set triangl_mesh bound_type cut neumann

set writer triangl_mesh vtk on
set writer triangl_mesh vtk file_name cube_triangl

set solver mesh_type triangular

run
