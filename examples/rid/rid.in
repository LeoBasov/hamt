# First test

read triangl_mesh rid.msh

set triangl_mesh bound_value circle_r 100
set triangl_mesh bound_value circle_l 300

set triangl_mesh bound_value buttom 0
set triangl_mesh bound_value right 0

set triangl_mesh bound_value left 0
set triangl_mesh bound_value top 0

set triangl_mesh bound_type buttom neumann
set triangl_mesh bound_type right neumann

set triangl_mesh bound_type left neumann
set triangl_mesh bound_type top neumann

set writer triangl_mesh vtk on
set writer triangl_mesh vtk file_name rid

set solver mesh_type triangular

run
