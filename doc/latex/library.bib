	file = {Yang et al. - 2020 - Vibrational–translational relaxation in nitrogen d.pdf:/home/arwen/baso_le/Zotero/storage/FUE87HHW/Yang et al. - 2020 - Vibrational–translational relaxation in nitrogen d.pdf:application/pdf},
}

