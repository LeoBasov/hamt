@article{gorji_kinetic_2012,
	title = {A {Kinetic} {Model} for {Gas} {Mixtures} {Based} on a {Fokker}-{Planck} {Equation}},
	volume = {362},
	issn = {1742-6596},
	url = {https://iopscience.iop.org/article/10.1088/1742-6596/362/1/012042},
	doi = {10.1088/1742-6596/362/1/012042},
	abstract = {We present a generalized nonlinear Fokker–Planck equation, which describes the dynamics of rareﬁed monatomic gas mixture ﬂows. The devised kinetic model leads to correct transfer of energy and momentum between gas species and to consistent evolution of molecular stresses and heat ﬂuxes with respect to the generalized Boltzmann equation. Thus, the correct diﬀusion coeﬃcient together with the mixture viscosity and mixture heat conductivity coeﬃcients are obtained. The strength of the presented model lies on the computational eﬃciency, which is due to the fact that the resulting stochastic processes are continuous in time. Therefore, unlike in Direct Simulation Monte Carlo (DSMC), here simulated particles do not collide with each other, but move along independent continuous stochastic paths. Another aspect of the new Fokker–Planck model equation is that here the eﬀect of collisions is described via drift and diﬀusion type processes. Accordingly, a scheme can be derived for which the time step size limitation of the corresponding numerical simulation becomes independent of the Knudsen number. Consequently, this leads to more eﬃcient simulations, especially in low or intermediate Knudsen numbers. Results are presented for helium-argon mixture in a one dimensional geometry. The calculated mixture viscosity is found to be in accordance with experimental data, which reveals the accuracy and relevance of the approach.},
	language = {en},
	urldate = {2022-02-18},
	journal = {Journal of Physics: Conference Series},
	author = {Gorji, Hossein and Jenny, Patrick},
	month = may,
	year = {2012},
	pages = {012042},
	file = {Gorji and Jenny - 2012 - A Kinetic Model for Gas Mixtures Based on a Fokker.pdf:/home/arwen/baso_le/Zotero/storage/WGPCIJPI/Gorji and Jenny - 2012 - A Kinetic Model for Gas Mixtures Based on a Fokker.pdf:application/pdf},
}

