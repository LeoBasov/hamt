@inproceedings{pfeiffer_extension_2019,
	address = {Glasgow, UK},
	title = {Extension of particle-based {BGK} models to polyatomic species in hypersonic flow around a flat-faced cylinder},
	url = {http://aip.scitation.org/doi/abs/10.1063/1.5119596},
	doi = {10.1063/1.5119596},
	abstract = {The simulation of gas and plasma ﬂows in the rareﬁed up to the continuum regime are of great interest to a multitude of applications ranging from atmospheric entry to electric propulsion. Large density gradients prohibit the use of a single method such as the well-established Direct Simulation Monte Carlo (DSMC) method due to excessive computational requirements in the transitional and continuum regime. For this purpose, the Bhatnagar-Gross-Krook (BGK) collision operator is implemented in the coupled three-dimensional Particle-in-Cell and DSMC code PICLas. On the one hand, the BGK method oﬀers the advantage that collision processes do not have to be resolved in comparison to DSMC. On the other hand, the method can be readily implemented and coupled with an existing particle code. The ellipsoidal statistical and Shakhov target distributions are employed in the BGK method to achieve the correct Prandtl number through a modiﬁcation of the pressure tensor and heat ﬂux, respectively. The randomwalk Metropolis-Hastings algorithm is utilized to eﬃciently sample from the target distribution, where the acceptance-rejection method is not applicable. The method is also extended to polyatomic molecules. To verify the implementation, simulation results of a hypersonic, non-reactive carbon dioxide ﬂow around a ﬂat-faced cylinder are compared to DSMC results. Additionally, the computational requirements between the BGK-based models and the DSMC method are compared to demonstrate the substantial decrease in the required computational eﬀort for BGK.},
	language = {en},
	urldate = {2021-07-08},
	author = {Pfeiffer, Marcel and Nizenkov, Paul and Fasoulas, Stefanos},
	year = {2019},
	pages = {100001},
	file = {Pfeiffer et al. - 2019 - Extension of particle-based BGK models to polyatom.pdf:/home/arwen/baso_le/Zotero/storage/6KVSAEFY/Pfeiffer et al. - 2019 - Extension of particle-based BGK models to polyatom.pdf:application/pdf},
}

